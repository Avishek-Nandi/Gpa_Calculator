Manager:
    WelcomeScreen:
    MenuScreen:
    CseCGPA:
    MatCGPA:
    PhyCGPA:
    EngCGPA:

<WelcomeScreen>:
    name: 'WelcomeScreen'

    BoxLayout:
        orientation: 'vertical'
        size: root.width , root.height

        padding: 10
        spacing: 20

        Label:
            text:""
            size_hint: (1,0.15)


        Image:
            source: 'data/icons/logo.png'
            pos_hint: {'center_x':0.5}
            size_hint: (1,1)
            padding: 10

        Button:
            text: "Get Started"
            pos_hint: {'center_x':0.5}
            size_hint: (.4,0.2)
            font_size: '20sp'
            on_press: root.get_pressed()
            on_release: app.root.current = "MenuScreen"


        Label:
            text:"Developed by, Avishek Nandi"
            font_size: '12sp'
            pos_hint: {'center_x':0.8}
            size_hint: (0.3,0.2)



<MenuScreen>:
    name: 'MenuScreen'

    BoxLayout:
        orientation: 'vertical'
        size: root.width , root.height

        padding: 10
        spacing: 20

        BoxLayout:
            orientation: 'horizontal'
            size_hint: (1,0.2)

            Label:
                text:"Course Selection"
                bold: True
                font_size: '30sp'
                size_hint: (0.25,1)
                pos_hint: {'center_x':1}
            Label:
                size_hint: (0.35,1)
            Button:

                background_normal: ""
                background_color: (61/255, 61/255, 61/255, 1)
                size_hint: (0.1,1)
                pos_hint: {'center_x':0.1}
                on_press: root.pressed()
                on_release: app.root.current = "WelcomeScreen"

                Image:
                    id: back_image
                    source: 'data/icons/back.png'
                    width: 50
                    center_x: self.parent.center_x
                    center_y: self.parent.center_y

        Label:
            text:""
            size_hint: (0.3,0.1)

        BoxLayout:
            orientation: 'vertical'
            spacing: 20

            Button:
                text: "CSE110"
                size_hint: (1,0.2)
                font_size: '20sp'
                on_press: root.cse_pressed()
                on_release: app.root.current = 'CseGPA'
            Button:
                text: "MAT110"
                size_hint: (1,0.2)
                font_size: '20sp'
                on_press: root.mat_pressed()
                on_release: app.root.current = 'MatGPA'

            Button:
                text: "PHY111"
                size_hint: (1,0.2)
                on_press: root.phy_pressed()
                on_release: app.root.current = 'PhyGPA'
                font_size: '20sp'

            Button:
                text: "ENG101"
                size_hint: (1,0.2)
                on_press: root.eng_pressed()
                on_release: app.root.current = 'EngGPA'
                font_size: '20sp'



        Label:
            text:""
            size_hint: (0.3,0.2)



